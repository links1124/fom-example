<?xml version="1.0" encoding="UTF-8"?>
<foms>
	<fom name="example5" config="com.fom.examples.TextZipImporterConfig">
		<remark>使用自定义pool的方式将本地指定目录下text文本的zip包解析导入Oracle库</remark>
		<src.uri>${webapp.root}/source</src.uri>
		<src.pattern>test.zip</src.pattern>
		<src.delMatchFail>false</src.delMatchFail>
		<scanner>com.fom.context.scanner.LocalScanner</scanner>
		<scanner.cron>0 0/15 * * * ?</scanner.cron>
		<context>com.fom.examples.Example5</context>
		<thread.min>10</thread.min>
		<thread.max>10</thread.max>
		<thread.aliveTime>30</thread.aliveTime>
		<thread.overTime>86400</thread.overTime>
		<thread.cancellable>false</thread.cancellable>
		<extends>
			<importer.batch>5000</importer.batch>
			<zip.entryPattern>.\.bcp</zip.entryPattern>
		</extends>
	</fom>
</foms>